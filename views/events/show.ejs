<h2 class="page-header"><%= event.name %></h2>
<img src="<%= event.imgPath %>">
<div>Venue:  <%= event.venue.name %></div>
<div>Address:  <%= event.venue.address %></div>
<div>Date:  <%= event.date.toDateString() %></div>
<div>Time: <%= event.startTime %> - <%= event.endTime %></div>
<div>Tickets:  <a href="<%= event.ticketLink %>" target="_blank" rel="noopener noreferrer">Event</a></div>
<div>Desc:  <%= event.desc %></div>

<div class="btn-row">
    <a class="btn btn-primary" href="/events/<%= event.id %>/edit">Edit</a>
    
    <a class="btn btn-primary" href="/venues/<%= event.venue.id %>/">View Venue</a>
    <%- include('../partials/deleteForm', { url: `/events/${event.id}` }) %>
</div>
